  "bugs": {
    "url": "https://github.com/ashermanli/phonebook-backend/issues"
  },
  "homepage": "https://github.com/ashermanli/phonebook-backend#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
